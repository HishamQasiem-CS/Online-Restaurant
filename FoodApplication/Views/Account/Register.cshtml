<body>
    <div class="page-wrapper bg-gra-02 p-t-130 p-b-100 font-poppins">
        <div class="wrapper wrapper--w680">
            <div class="card card-4">
                <div class="card-body">
                    <h2 class="title">Registration HQ Restaurant</h2>
                    <form asp-action="Register" asp-controller="Account" method="post">
                        <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label" asp-for="Email"></label>
                                    <input asp-for="Email" class="input--style-4"  >
                                    <span style="color: red;font-size:20px;" asp-validation-for="Email"></span>
                                </div>
                            </div>
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label" asp-for="Name"></label>
                                    <input asp-for="Name" class="input--style-4" >
                                     <span style="color: red;font-size:20px;" asp-validation-for="Name"></span>
                                </div>
                            </div>
                        </div>
                        <div id="date-picker-example" class="md-form md-outline input-with-post-icon datepicker" inline="true">
                            <label asp-for="BirthDate"></label>
                            <input class="form-control" asp-for="BirthDate" />
                            <i class="fas fa-calendar input-prefix"></i>
                            <span style="color: red;font-size:20px;" asp-validation-for="BirthDate"></span>
                        </div>
              
                           
                        <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label" asp-for="Password"></label>
                                        <input asp-for="Password" class="input--style-4" type="password" >
                                    <span style="color: red;font-size:20px;" asp-validation-for="Password" ></span>
                                </div>
                                </div>
                            </div>
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label" asp-for="ConfirmPassword"></label>
                                            <input asp-for="ConfirmPassword" class="input--style-4" type="password" >
                                             <span style="color: red;font-size:20px;" asp-validation-for="ConfirmPassword"></span>
                                        </div>
                                    </div>

                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label" asp-for="Country"></label>
                                            
                                         
                                            @Html.DropDownList("Country",Html.GetEnumSelectList<Country>(),"Choose the Country Availably" , new{
                                            @class="input--style-4"
                                            })


                                    <span style="color: red;font-size:20px;" asp-validation-for="Country"></span>
                                </div>
                            </div>
                        </div>
                        <div class="input-group">
                            <label class="label" asp-for="City"></label>
                            
                                        @Html.DropDownList("City",Html.GetEnumSelectList<City>(),"Choose the City Availably", new{
                                @class="input--style-4"
                                        })
                            <span style="color: red;font-size:20px;" asp-validation-for="City"></span>
                        </div>
                                <div class="input-group">
                                    <label class="label" asp-for="Geunder"></label>
                                    
                                        @Html.DropDownList("Geunder",Html.GetEnumSelectList<Geunder>(),"Choose the Geunder", new{
                                @class="input--style-4"
                                        })
                            <span style="color: red;font-size:20px;" asp-validation-for="Geunder"></span>
                                </div>
                                 <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label" asp-for="Address"></label>
                                        <input asp-for="Address" class="input--style-4" type="text" name="Address">
                                    <span style="color: red;font-size:20px;" asp-validation-for="Address"></span>
                                </div>
                                </div>
                            </div>
                            <div class="row row-space">
                                <div class="col-2">
